{
    "name": "vodolaz.pro",
    "description": "Приложение для расчета данных, связанных с водолазами.",
    "version": "1.0.0",
    "main": "public/electron.js",
    "private": true,
    "homepage": "./",
    "author": {
        "name": "Nikita",
        "email": "nikitamashkov13@gmail.com"
    },
    "dependencies": {
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/lodash": "^4.14.202",
        "@types/node": "^16.18.61",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "electron-is-dev": "^2.0.0",
        "electron-router-dom": "^1.0.5",
        "lodash": "^4.17.21",
        "mobx": "^6.11.0",
        "mobx-react-lite": "^4.0.5",
        "node-sass": "^7.0.3",
        "openai": "^4.20.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.19.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "concurrently": "^8.2.2",
        "electron": "^27.0.4",
        "electron-builder": "^24.6.4",
        "eslint": "8.6.0",
        "import-sort-style-module": "^6.0.0",
        "prettier": "^2.3.1",
        "prettier-plugin-import-sort": "^0.0.7",
        "react-scripts": "5.0.1",
        "typescript": "^4.9.5",
        "wait-on": "^7.2.0"
    },
    "scripts": {
        "start": " react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "dev": "concurrently \"npm start\" \"wait-on tcp:127.0.0.1:3003 && electron .\"",
        "electron-build": "npm run build && electron-builder"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            "last 1 electron version",
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 electron version",
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module"
        }
    },
    "build": {
        "appId": "com.vodolaz.pro",
        "productName": "Vodolaz.pro",
        "copyright": "Copyright © 2023 ${author}",
        "mac": {
            "category": "public.app-category.utilities"
        },
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        }
    }
}